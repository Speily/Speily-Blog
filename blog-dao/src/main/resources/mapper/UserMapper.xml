<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.speily..UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.speily.entity.User">
        <id column="id" property="id" />
        <result column="login_name" property="loginName" />
        <result column="mobile" property="mobile" />
        <result column="password" property="password" />
        <result column="user_name" property="userName" />
        <result column="sex" property="sex" />
        <result column="age" property="age" />
        <result column="email" property="email" />
        <result column="position" property="position" />
        <result column="city" property="city" />
        <result column="create_time" property="createTime" />
        <result column="create_by" property="createBy" />
        <result column="update_time" property="updateTime" />
        <result column="update_by" property="updateBy" />
        <result column="status" property="status" />
        <result column="delete_flag" property="deleteFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, login_name as loginName, mobile, password, user_name, sex, age, email, position, city, create_time, create_by, update_time, update_by, status, delete_flag
    </sql>

</mapper>
